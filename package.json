{
	"type": "module",
	"name": "next-template-16",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev --turbopack",
		"build": "next build",
		"build:analyze": "ANALYZE=true next build",
		"storybook": "storybook dev -p 6006 --no-open",
		"storybook:build": "storybook build",
		"start": "next start",
		"typecheck": "tsc --noEmit --project configs/tsconfig/tsconfig.test.json",
		"lint": "biome lint .",
		"lint:fix": "biome lint . --write",
		"format": "biome format . && prettier .",
		"format:fix": "biome format . --write && prettier --write .",
		"check": "biome check . && prettier .",
		"check:fix": "biome check . --write && prettier --write .",
		"test": "vitest",
		"test:vrt": "pnpm storybook:build -o tests/test-results/storybook-static && docker compose exec -e SNAPSHOT_PATH=$SNAPSHOT_PATH vrt npx playwright@1.56.1 test",
		"test:vrt:update": "pnpm storybook:build -o tests/test-results/storybook-static && docker compose exec -e SNAPSHOT_PATH=$SNAPSHOT_PATH vrt npx playwright@1.56.1 test --update-snapshots",
		"test:vrt:report": "docker compose exec vrt npx playwright@1.56.1 show-report tests/test-results/vrt-report --host 0.0.0.0 --port 9001",
		"swagger": "pnpm swagger:build && pnpm swagger:serve",
		"swagger:build": "openapi-generate-html -i docs/api/v1/openapi.json --theme light --ui stoplight -o docs/api/v1/stoplight/index.html",
		"swagger:serve": "serve -l 6608 docs/api/v1/stoplight",
		"orval": "orval --config ./configs/orval/orval.config.ts",
		"generate": "plop",
		"prepare": "lefthook install"
	},
	"dependencies": {
		"@hookform/resolvers": "5.2.2",
		"clsx": "2.1.1",
		"next": "16.0.10",
		"react": "19.2.3",
		"react-dom": "19.2.3",
		"react-hook-form": "7.56.4",
		"tailwind-merge": "3.4.0",
		"zod": "4.1.13"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.4",
		"@commitlint/cli": "19.8.1",
		"@faker-js/faker": "10.1.0",
		"@modelcontextprotocol/sdk": "1.24.3",
		"@next/bundle-analyzer": "16.0.10",
		"@playwright/test": "1.56.1",
		"@storybook/addon-a11y": "10.0.6",
		"@storybook/addon-links": "10.0.6",
		"@storybook/addon-vitest": "10.0.6",
		"@storybook/nextjs-vite": "10.0.6",
		"@tailwindcss/cli": "4.1.17",
		"@tailwindcss/postcss": "4.1.17",
		"@tailwindcss/vite": "4.1.17",
		"@testing-library/jest-dom": "6.9.1",
		"@testing-library/react": "16.3.0",
		"@testing-library/user-event": "14.6.1",
		"@types/node": "22.13.14",
		"@types/react": "19.2.3",
		"@types/react-dom": "19.2.3",
		"@vitejs/plugin-react": "5.1.1",
		"jsdom": "27.2.0",
		"lefthook": "1.13.0",
		"msw": "2.12.4",
		"openapi-generate-html": "0.5.3",
		"orval": "7.17.0",
		"plop": "4.0.4",
		"prettier": "3.6.2",
		"prettier-plugin-tailwindcss": "0.6.14",
		"serve": "14.2.5",
		"storybook": "10.0.6",
		"storybook-addon-pseudo-states": "10.0.6",
		"tailwindcss": "4.1.17",
		"tsx": "4.20.6",
		"typescript": "5.9.2",
		"vite": "7.1.5",
		"vitest": "4.0.15"
	},
	"packageManager": "pnpm@10.24.0"
}
